<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="org.yoon.mapper.GBoardAttachMapper">
 	<insert id="insert">
 		insert into g_attach (uuid, uploadpath, filename, filetype, bno) values
 		(#{uuid}, #{uploadPath}, #{fileName}, #{fileType}, #{bno} )
 	</insert>
 	
 	<delete id="delete">
 		delete from g_attach where uuid = #{uuid}
 	</delete>
 	
 	<select id="findByBno" resultType="org.yoon.domain.GBoardAttachVO">
 		select * from g_attach where bno = #{bno}
 	</select>
 	
 	<delete id="deleteAll">
 		delete g_attach where bno = #{bno}
 	</delete>
 </mapper>